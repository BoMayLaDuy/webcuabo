<body>
  <h2>Link4m Auto Fill</h2>
  <script>
    (function() {
      const myCode = "loadstring(game:HttpGet("https://rawscripts.net/raw/Hunty-Zombie-UPDATE-1.6-OP-Autofarm-keyless-50501"))()";
      function autoFill() {
          const input = document.querySelector("input[type='text']");
          const btn = document.querySelector("button, input[type='submit']");
          if (input && myCode) input.value = myCode;
          if (btn) btn.click();
      }
      window.addEventListener('load', () => {
          setTimeout(autoFill, 1500);
      });
    })();
  </script>
